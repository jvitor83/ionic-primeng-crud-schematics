

.ui-paginator-last	
{
    display: none;
}

.toolbar-without-background {
  --background: none;
}

.ui-widget-header {
  border: none;
  background: none;
  background-color: none;
}

.ui-paginator-element {
  width: 2.7em !important;
  height: 2.7em !important;
}

.ui-paginator-pages {
  height: none !important;
}

.ui-paginator a {
  color: white;
}

.ui-state-active {
  color: red !important;
}

.ui-state-default {
  border: none !important;
  background: none !important;
  //color: white !important;
}

.ui-table .ui-table-thead > tr > th, .ui-table .ui-table-tfoot > tr > td {
  background: var(--ion-color-primary-shade, none);
  color: var(--ion-color-primary-contrast, white);
}

.ui-column-title {
  padding: 0px !important;
  min-width: 120px !important;
}

